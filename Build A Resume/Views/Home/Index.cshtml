@{
    ViewData["Title"] = "Home Page";
}

<!DOCTYPE html>
<html lang="en">



<body>
    

    <section class="features">
        <div class="feature">
            <a href="@Url.Action("Create", "PersonalInformations")">

                <img src="./Resume/Resume1.png" style="width:80px;height:140px;" alt="Feature 1 Icon">
                <h2>Easy to Use</h2>
                <p>Create your resume with a user-friendly interface.</p>
            </a>
        </div>
        <div class="feature">
            <img src="data:image/jpeg;base64,/9j/4AAQSkZJRgABAQAAAQABAAD/2wCEAA
                 oHCBIVFBgVExQZFRgaGxweGxobGh0eHhsbHhwaHBsiGBsbIC0kHCIpHhocJTclK
                 S4wNDQ0GyQ5PzkyPi0yNDIBCwsLEA8QGxISHjApIyA1NDIyMjIyNDAyNDA2PjsyN
                 " style="width:80px;height:140px;" alt="Feature 2 Icon">
            <h2>Customizable</h2>
            <p>Customize your resume with templates and styles.</p>
        </div>
        <div class="feature">
            <img src="./Resume/Resume 3.png" style="width:80px;height:140px;" alt="Feature 3 Icon">
            <h2>Download PDF</h2>
            <p>Download your resume as a PDF for easy sharing.</p>
        </div>
    </section>

    <section class="testimonial">
        <h2>What Our Users Say</h2>
        <div class="quote">
            <p>
                <a href="@Url.Action("Create", "PersonalInformations")">Get started with us To Create Your Resume</a>
            </p>
            <p class="author">- Srikanth</p>
        </div>
    </section>

   
</body>

</html>
<style>
    /* Style the "Get Started" button */
    .btn {
        display: inline-block;
        padding: 10px 20px;
        background-color: #27ae60;
        color: #fff;
        text-decoration: none;
        border-radius: 5px;
        transition: background-color 0.3s;
    }

        .btn:hover {
            background-color: #1e8449;
        }

    /* Style the features section */
    .features {
        display: flex;
        justify-content: space-around;
        margin: 60px 0;
    }

    .feature {
        text-align: center;
        margin: 0 20px;
    }

        .feature h2 {
            font-size: 24px;
            margin: 10px 0;
        }

    /* Style the testimonial section */
    .testimonial {
        text-align: center;
        background-color: #f2f2f2;
        padding: 60px 0;
    }

    .quote {
        font-style: italic;
        margin-bottom: 20px;
    }

    .author {
        font-weight: bold;
    }

    

    /* Add more custom styles as needed */

</style>